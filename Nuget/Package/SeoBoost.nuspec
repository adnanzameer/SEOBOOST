<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SeoBoost</id>
    <title>SeoBoost</title>
    <description>SEOBOOST for Episerver - Improves SEO ranking of the website with HtmlHelper methods for Internationalization i.e hreflang attributes (Alternate links), canonical tag (rel canonical), Breadcrumbs and editable robots.txt</description>
    <releaseNotes>removed support simple url/ external url  in canonical tags and hreflang attributes. </releaseNotes>
    <version>1.6.0</version>
    <authors>Adnan Zameer</authors>
    <owners>Adnan Zameer</owners>
    <licenseUrl>https://raw.githubusercontent.com/adnanzameer/SEOBOOST/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/adnanzameer/SEOBOOST/</projectUrl>
   	<iconUrl>https://raw.githubusercontent.com/adnanzameer/SEOBOOST/master/boost.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>seo hreflang alternate links canonical tag breadcrumbs robots.txt internationalization</tags>
    <dependencies>
      <dependency id="EPiServer.CMS.UI" version="[11.0, 12.0)" />
    </dependencies>
  </metadata>
</package>